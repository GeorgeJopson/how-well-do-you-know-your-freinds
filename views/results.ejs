<%- include('partials/header'); %>
<div class="group resultsGroup">
  <h3><%= message %></h3>
  <h1><%= output %></h1>
</div>
<br>
<% if(message=="Your score (how many answers you guessed correctly) is..."){ %>
  <div class="group answers">
    <h3>Your friend's answers</h3>
    <% for (var i = 0; i < friendsResult.length; i++) { %>
      <p><%= questions[i][0] %> : <%= questions[i][1][friendsResult[i]-1] %></p>
    <% } %>
  </div>
<% } %>

<%- include('partials/footer'); %>
